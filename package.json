{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Shubidumdu/how-fast-wasm.git",
  "author": "Alan Seo <54790378+Shubidumdu@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "my-wasm": "file:./pkg"
  },
  "devDependencies": {
    "@webpack-cli/generators": "^2.4.2",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "description": "My webpack project",
  "scripts": {
    "build": "yarn build:wasm && webpack --mode=production --node-env=production",
    "build:dev": "yarn build:wasm && webpack --mode=development",
    "build:prod": "yarn build",
    "build:wasm": "wasm-pack build wasm --out-dir ../pkg",
    "watch:wasm": "cargo watch -C wasm -s \"yarn build:wasm\"",
    "serve": "yarn watch:wasm & webpack serve"
  }
}
